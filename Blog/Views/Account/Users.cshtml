@model IEnumerable<Blog.Models.Person>

<div class="row">
    <div class="box">
        <div class="col-lg-12">
            <hr>
            <h2 class="intro-text text-center">
                <strong>Все пользователи</strong>
            </h2>
            <hr>
            <table class="table">
                <tr>
                    <td>ID</td>
                    <td>Роль</td>
                    <td>Логин</td>
                    <td>Имя</td>
                    <td>Фамилия</td>
                    <td>Email</td>
                </tr>
                @foreach (var user in Model)
                {
        <tr>
            <td>@user.Id</td>
            <td>@user.RoleId</td>
            <td>@user.Login</td>
            <td>@user.FirstName</td>
            <td>@user.LastName</td>
            <td>@user.Email</td>
        </tr>}
            </table>
        </div>
    </div>
</div>