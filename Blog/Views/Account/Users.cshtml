@model IEnumerable<Blog.Models.Person>

<div class="row">
    <div class="box">
        <div class="col-lg-12">
            <hr>
            <h2 class="intro-text text-center">
                <strong>Все пользователи</strong>
            </h2>
            <hr>
            <table class="table">
                <tr>
                    <td>ID</td>
                    <td>Роль</td>
                    <td>Логин</td>
                    <td>Пароль</td>
                    <td>Имя</td>
                    <td>Фамилия</td>
                    <td>Email</td>
                </tr>
                @foreach (var user in Model)
                {
                    <tr>
                        <td>@user.PersonId</td>
                        <td>@user.PersonRoleId</td>
                        <td>@user.PersonLogin</td>
                        <td>@user.PersonPassword</td>
                        <td>@user.PersonFirstName</td>
                        <td>@user.PersonLastName</td>
                        <td>@user.PersonEmail</td>
                    </tr>
                }
            </table>
        </div>
    </div>
</div>